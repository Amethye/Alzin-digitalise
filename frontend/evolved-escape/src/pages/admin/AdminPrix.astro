---
import Layout from "@layouts/Layout.astro";
import AdminPrix from "../../components/AdminPrix";
import RequireRole from "@components/RequireRole";
---

<Layout title="Admin - Prix">
  <main class="flex min-h-screen justify-center bg-white px-4 py-6 sm:px-6 sm:py-8">
    <RequireRole allowedRoles={['admin']} client:load>
      <AdminPrix client:load />
    </RequireRole>
  </main>
</Layout>
