{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "paths": {
      "@styles/*" : ["./src/styles/*"],
      "@layouts/*" : ["./src/layouts/*"],
      "@images/*" : ["./src/assets/images/*"],
      "@icons/*" : ["./src/icons/*"],
      "@components/*" : ["./src/components/*"]
    }
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
